---
import { setPagemeta } from "@grepco/astro-pagemeta/runtime";

setPagemeta({
    ctx: Astro,
    metadata: {
        description: "Overridden description",
        title: "Overridden Title"
    }
});
---

<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
    </head>
    <body>
        <p>Page with overridden metadata</p>
    </body>
</html>
