{
    "name": "@grepco/astro-pagemeta",
    "version": "0.0.0",
    "description": "An Astro integration for setting your pages' meta tags",
    "keywords": [
        "astro",
        "astro-integration",
        "seo"
    ],
    "homepage": "https://github.com/zemccartney/astro-pagemeta",
    "bugs": {
        "url": "https://github.com/zemccartney/astro-pagemeta/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zemccartney/astro-pagemeta"
    },
    "license": "MIT",
    "author": "Zack McCartney <zack@grepco.net>",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./runtime": "./runtime-stub.js"
    },
    "files": [
        "dist",
        "runtime-stub.js"
    ],
    "scripts": {
        "deps": "ncu -p pnpm -i --format group --cooldown 3",
        "fmt": "prettier . --write",
        "fmt:check": "prettier . --check",
        "lint": "eslint .",
        "lint:debug": "eslint --inspect-config",
        "lint:fix": "eslint . --fix",
        "test": "vitest --run",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@inox-tools/aik-mod": "^0.12.0",
        "@inox-tools/inline-mod": "^2.3.1",
        "astro-integration-kit": "^0.19.1",
        "rehype": "^13.0.2",
        "rehype-meta": "^4.0.1"
    },
    "devDependencies": {
        "@astrojs/node": "^9.5.2",
        "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
        "@eslint/compat": "^2.0.1",
        "@eslint/js": "^9.39.2",
        "@eslint/json": "^0.14.0",
        "@inox-tools/astro-tests": "^0.8.0",
        "@types/hast": "^3.0.4",
        "@types/node": "^25.0.9",
        "astro": "^5.16.11",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-astro": "^1.5.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-package-json": "^0.88.1",
        "eslint-plugin-perfectionist": "^5.3.1",
        "eslint-plugin-unicorn": "^62.0.0",
        "globals": "^17.0.0",
        "npm-check-updates": "^19.3.1",
        "prettier": "^3.8.0",
        "prettier-plugin-astro": "^0.14.1",
        "rehype-parse": "^9.0.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.53.0",
        "unified": "^11.0.5",
        "vitest": "^4.0.17"
    },
    "peerDependencies": {
        "astro": "5.x.x"
    },
    "packageManager": "pnpm@10.28.2",
    "engines": {
        "node": "24"
    },
    "publishConfig": {
        "engines": {
            "node": "18.20.8 || ^20.3.0 || >=22.0.0"
        }
    }
}
